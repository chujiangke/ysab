# !/bin/bash
prefix='ysab_linux_'
version='0.4.2'
echo '------------------------------------------------'
echo 'ysab: The version you will install is' $version
echo '------------------------------------------------'
sleep 2
rm -rf $prefix$version.tgz
wget https://github.com/yunsonbai/ysab/releases/download/v$version/$prefix$version.tgz
tar -zxvf $prefix$version.tgz
mv $prefix$version ysab
chmod +x ysab
mv ysab /usr/local/bin/
rm -rf $prefix$version.tgz